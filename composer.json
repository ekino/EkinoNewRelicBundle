{
    "name": "temando/newrelic-bundle",
    "type": "symfony-bundle",
    "description": "Integrate New Relic into Symfony2 with ignore transactions update",
    "keywords": ["metric", "performance", "monitoring"],
    "homepage": "https://github.com/temando/EkinoNewRelicBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@ekino.com",
            "homepage": "http://ekino.com"
        },
        {
            "name": "Anjali Rana",
            "email": "anjali.rana@temando.com",
            "homepage": "https://github.com/anjalirana"
        }
    ],
    "require-dev": {
        "silex/silex": "~1.0",
        "symfony/framework-bundle": "~2.2",
        "symfony/console": "~2.2",
        "twig/twig": "1.*"
    },
    "suggest": {
        "sonata-project/block-bundle": "2.2.*@dev"
    },
    "autoload": {
        "psr-0": { "Ekino\\Bundle\\NewRelicBundle": "" }
    },
    "target-dir": "Ekino/Bundle/NewRelicBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
